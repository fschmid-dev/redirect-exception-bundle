  "description": "Adds a special RedirectException with a Subscriber, to enable redirects from outside of a controller.",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "FSchmid",
      "email": "info@fschmid.dev"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=8",
    "symfony/flex": "^2",
    "symfony/http-kernel": "6.3.*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "FSchmidDev\\RedirectExceptionBundle\\": "src/"
    }
  }
}
